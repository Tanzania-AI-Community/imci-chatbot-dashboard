{
  "name": "imci-dashboard",
  "description": "IMCI Dashboard - A comprehensive dashboard for managing IMCI chatbot data and analytics.",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf --glob **/node_modules **/dist **/.next pnpm-lock.yaml **/.tsbuildinfo",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "check": "pnpm lint && pnpm typecheck && pnpm format:check",
    "shadcn:add": "pnpm dlx shadcn@latest add",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "dotenv -e .env -- tsx db/migrate.ts",
    "db:seed": "dotenv -e .env -- tsx db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:init": "pnpm run db:migrate && pnpm run db:seed"
  },
